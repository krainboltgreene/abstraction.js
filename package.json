{
  "name": "abstraction",
  "version": "6.0.0",
  "description": "A library for modeling abstract business logic or wrapping data structures.",
  "main": "transpiled/source/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf './transpiled/' && mkdir './transpiled/' && babel './source/**/index.js' -d './transpiled/'",
    "test": "mocha --compilers js:babel-register './source/**/test.js'",
    "start": "npm-run-all --parallel watch:build watch:test",
    "lint": "eslint './source/'",
    "watch": "npm-run-all --parallel watch:test watch:lint watch:build",
    "watch:build": "chokidar './source/' --initial --command 'npm run build --silent'",
    "watch:test": "chokidar './source/' --initial --command 'npm run test --silent'",
    "watch:lint": "chokidar './source/' --initial --command 'npm run lint --silent'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krainboltgreene/abstraction.js.git"
  },
  "keywords": [
    "abstract",
    "model",
    "orm",
    "odm",
    "presenter",
    "serializer",
    "decorator"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/krainboltgreene/abstraction.js/issues"
  },
  "homepage": "https://github.com/krainboltgreene/abstraction.js#readme",
  "devDependencies": {
    "babel": "^6",
    "babel-cli": "^6",
    "babel-core": "^6",
    "babel-eslint": "^6",
    "babel-plugin-transform-es2015-computed-properties": "^6",
    "babel-plugin-transform-es2015-destructuring": "^6",
    "babel-plugin-transform-es2015-function-name": "^6",
    "babel-plugin-transform-es2015-literals": "^6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6",
    "babel-plugin-transform-es2015-parameters": "^6",
    "babel-plugin-transform-es2015-spread": "^6",
    "babel-plugin-transform-es2015-sticky-regex": "^6",
    "babel-plugin-transform-es2015-unicode-regex": "^6",
    "babel-plugin-transform-export-extensions": "^6",
    "babel-plugin-transform-function-bind": "^6",
    "babel-plugin-transform-member-expression-literals": "^6",
    "babel-plugin-transform-object-assign": "^6",
    "babel-plugin-transform-object-rest-spread": "^6",
    "babel-plugin-transform-object-set-prototype-of-to-assign": "^6",
    "babel-plugin-transform-property-literals": "^6",
    "babel-plugin-transform-proto-to-assign": "^6",
    "chai": "^3",
    "chokidar-cli": "^1",
    "eslint": "^2",
    "eslint-plugin-babel": "^3",
    "eslint-plugin-immutable": "^1",
    "eslint-plugin-import": "^1",
    "eslint-plugin-mocha": "^3",
    "eslint-plugin-node": "^1",
    "greenkeeper": "^2",
    "mocha": "^2",
    "moment": "^2",
    "npm": "^3",
    "npm-run-all": "^2"
  },
  "peerDependency": {
    "moment": "^2"
  },
  "dependencies": {
    "bunyan": "^1",
    "ramda": "0.21.0"
  }
}
